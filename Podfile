# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

def share_pods
	pod 'FSCalendar'
	pod 'RealmSwift'
end

target 'WAY' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
	share_pods

  # Pods for WAY

end

target 'MyWidgetExtension' do
  use_frameworks!
	share_pods
end


